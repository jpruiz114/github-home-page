{
  "name": "coco/github-home-page",
  "description": "GitHub Home Page extra elements",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Jean Paul Ruiz",
      "email": "jpruiz114@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/TheCocoTeam/source-watcher-core"
    }
  ],
  "require": {
    "ext-curl": "*",
    "ext-dom": "*",
    "ext-json": "*",
    "php": "~7.4",
    "vlucas/phpdotenv": "^4.1",
    "coco/source-watcher": "master@dev"
  },
  "autoload": {
    "psr-4": {
      "Coco\\GitHubHomePage\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Coco\\GitHubHomePage\\": "src/",
      "Coco\\GitHubHomePage\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  }
}
